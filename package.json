{
  "name": "mdbutton",
  "version": "0.0.6",
  "description": "MDButton is an easy to use button component",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --port 3000",
    "precompile": "npm run lint",
    "compile": "npm-run-all --parallel compile:source compile:postcss",
    "compile:postcss": "postcss  --use precss --use postcss-cssnext -d lib src/*.css",
    "compile:source": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "lint": "eslint src/**/*.jsx",
    "prepublish": "npm-run-all clean compile",
    "test:watch": "mocha --watch",
    "test": "mocha"
  },
  "repository": "https://github.com/Madadata/MDButton",
  "keywords": [
    "Button",
    "Madadata",
    "ES6"
  ],
  "author": "zhenghe",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "react": "15.1.0",
    "react-dom": "15.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-module-alias": "1.5.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "expect": "1.20.1",
    "expect-jsx": "2.6.0",
    "ghooks": "1.2.4",
    "ignore-styles": "^4.0.0",
    "mocha": "^2.5.3",
    "npm-run-all": "2.2.2",
    "postcss": "5.0.21",
    "postcss-cli": "2.5.2",
    "postcss-cssnext": "2.6.0",
    "postcss-loader": "0.9.1",
    "precss": "1.4.0",
    "react": "15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "15.1.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run prepublish"
    }
  }
}
